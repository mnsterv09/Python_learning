# coordenates by https://www.latlong.net/

my_city = ('Belo Horizonte', '-19.917299', '-43.934559')
biel_city = ('Tenerife', '28.291565', '-16.629129')
sergin_city = ('Connecticut', '41.603222', '-73.087746')
